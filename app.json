{
  "expo": {
    "name": "SwatchSetu",
    "slug": "swatchsetu",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2E7D32"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.humanmatrix.swatchsetu"
    },
    "android": {
      "package": "com.humanmatrix.swatchsetu",
      "adaptiveIcon": {
        "backgroundColor": "#2E7D32"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "your-eas-project-id"
      }
    },
    "plugins": [
      "expo-firebase-core"
    ]
  }
}
